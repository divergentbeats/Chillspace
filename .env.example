# Discord API Configuration
# Get these from https://discord.com/developers/applications

# Bot Token (for fetching servers where your bot is present)
REACT_APP_DISCORD_BOT_TOKEN=your_bot_token_here

# OAuth2 Application Credentials (for user authentication)
REACT_APP_DISCORD_CLIENT_ID=your_client_id_here
REACT_APP_DISCORD_CLIENT_SECRET=your_client_secret_here

# OAuth2 Redirect URI (must match the one configured in Discord Developer Portal)
REACT_APP_DISCORD_REDIRECT_URI=http://localhost:3001/auth/discord/callback
